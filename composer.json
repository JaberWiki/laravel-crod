{
  "name": "milwad/laravel-crod",
  "description": "Make easy & fast crud",
  "keywords": [
      "milwad",
      "laravel-crod",
      "easy crud"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
      {
          "name": "milwad khosravi",
          "email": "milwad.dev@gmail.com",
          "homepage": "https://milwad.ir",
          "role": "Developer"
      }
  ],
  "require": {
      "php": "^8.0|^8.1",
      "doctrine/dbal": "^3.3",
      "laravel/framework": "5.*|6.*|7.*|8.*|9.*"
  },
  "require-dev": {
    "orchestra/testbench": "7.0",
    "phpunit/phpunit": "9.5.20"
  },
  "autoload": {
      "psr-4": {
          "Milwad\\LaravelCrod\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Milwad\\LaravelCrod\\Tests\\": "tests"
      }
  },
  "config": {
     "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Milwad\\LaravelCrod\\LaravelCrodServiceProvider"
          ]
      }
  },
  "scripts": {
     "test": "vendor/bin/phpunit",
     "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  }
}
